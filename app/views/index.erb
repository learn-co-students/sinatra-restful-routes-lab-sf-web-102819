<h1>Welcome</h1>
<h2>View Your Recipes:</h2>

<% @recipes.each do |recipe|%>
    <ul>
        <li>
            <a href="/recipes/<%= recipe.id %>"><%= recipe.name %></a>
        </li>
        <li>
        <%= recipe.ingredients %>
        </li>
        <li>
        <%= recipe.cook_time %>
        </li>

    </ul>

<% end %>